# Local Lift backend environment configuration
# Copy to .env and adjust values for your environment.

LOCALLIFT_ENVIRONMENT=local
LOCALLIFT_LOG_LEVEL=INFO
LOCALLIFT_DATABASE_URL=postgresql+asyncpg://locallift:locallift_dev@localhost:5433/locallift
LOCALLIFT_DATABASE_ECHO=false
LOCALLIFT_TCAD_BASE_URL=https://gis.traviscountytx.gov/server1/rest/services/Boundaries_and_Jurisdictions/TCAD/MapServer/0
LOCALLIFT_TCAD_PAGE_SIZE=1000
LOCALLIFT_TCAD_CONCURRENCY=4
LOCALLIFT_REQUEST_TIMEOUT_SECONDS=30
LOCALLIFT_REQUEST_RETRY_ATTEMPTS=5
LOCALLIFT_REQUEST_RETRY_BACKOFF=0.5
LOCALLIFT_EXPORT_DIR=

# Contact Enrichment (Hunter.io)
# Get your API key from https://hunter.io/api
LOCALLIFT_HUNTER_IO_API_KEY=
LOCALLIFT_HUNTER_IO_RATE_LIMIT_PER_MINUTE=50
